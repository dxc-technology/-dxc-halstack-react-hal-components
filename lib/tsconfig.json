{
  "compilerOptions": {
    "declaration": true,
    "emitDeclarationOnly": true,
    "isolatedModules": true,
    "rootDir": "./src",
    "outDir": "./dist",
    "jsx": "preserve",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": false,
    "module": "esnext",
    "target": "ESNext",
    "moduleResolution": "Node"
  }
}
